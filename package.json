{
  "name": "vue-plasmo-test",
  "version": "0.0.1",
  "displayName": "好狗导航",
  "author": "haogou.cc",
  "description": "一个导航新窗口插件",
  "packageManager": "pnpm@8.2.0",
  "type": "module",
  "scripts": {
    "dev": "plasmo dev",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "extension:dev": "plasmo dev"
  },
  "dependencies": {
    "plasmo": "0.84.1",
    "vue": "^3.3.4",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.241",
    "@types/node": "20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "element-plus": "^2.5.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vue-tsc": "^1.8.5"
  },
  "manifest": {
    "name": "好狗导航",
    "description": "一个可以搜索影视资源的插件",
    "version": "1.0",
    "host_permissions": [
      "<all_urls>"
    ],
    "homepage_url": "https://github.com/iszmxw",
    "options_page": "options.html",
    "permissions": [
      "tabs",
      "storage",
      "clipboardWrite"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "manifest.json",
          "*.css"
        ]
      }
    ]
  }
}